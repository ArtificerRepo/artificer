<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="srampui">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="com.google.gwt.http.HTTP" />
	
	<entry-point class="org.overlord.sramp.ui.client.Application" />

	<!-- Impls -->
	<replace-with class="org.overlord.sramp.ui.client.ClientFactory">
		<when-type-is class="org.overlord.sramp.ui.client.IClientFactory" />
	</replace-with>

	<!-- Specify the paths for translatable code -->
	<source path="client" />
	<source path="shared" />

</module>
