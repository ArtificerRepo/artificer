<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xml:base="http://www.example.org/colors.owl">
	
	<owl:Ontology rdf:ID="Colors">
		<rdfs:label>Colors</rdfs:label>
		<rdfs:comment>Colors ontology</rdfs:comment>
	</owl:Ontology>
	
	<owl:Class rdf:ID="Primary">
		<rdfs:label>Primary</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:ID="Red">
		<rdfs:subClassOf rdf:resource="http://www.example.org/colors.owl#Primary" />
		<rdfs:label>Red</rdfs:label>
	</owl:Class>
	<owl:Class rdf:ID="Green">
		<rdfs:subClassOf rdf:resource="http://www.example.org/colors.owl#Primary" />
		<rdfs:label>Green</rdfs:label>
	</owl:Class>
	<owl:Class rdf:ID="Blue">
		<rdfs:subClassOf rdf:resource="http://www.example.org/colors.owl#Primary" />
		<rdfs:label>Blue</rdfs:label>
	</owl:Class>
	
</rdf:RDF>
