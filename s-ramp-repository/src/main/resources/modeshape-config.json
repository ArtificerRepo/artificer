{
    "name" : "S-RAMP Repository",
    "workspaces" : {
        "predefined" : ["otherWorkspace"],
        "default" : "default",
        "allowCreation" : true
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : false
        }
    },
    "sequencing" : {
        "removeDerivedContentWithOriginal" : true,
        "sequencers" : [
            {
                "description" : "XSD sequencer",
                "type" : "xsdsequencer",
                "pathExpressions" : [ "default:/artifact/xsd/(*)/(*)/(*)/*/jcr:content[@jcr:data] => default:/s-ramp/xsd/$1/$2/$3" ]
             },
             {
                "description" : "XML sequencer",
                "type" : "xmlsequencer",
                "pathExpressions" : [ "default:/artifact/xml/(*)/(*)/(*)/*/jcr:content[@jcr:data] => default:/s-ramp/xml/$1/$2/$3" ]
            }
        ]
    }
}